# Environment variables for Online Status Backend
# Copy this file to .env and fill in your values

# API Token for authentication (REQUIRED for production)
# Generate a secure random token, e.g.: openssl rand -hex 32
API_TOKEN=dev-token-change-me

# Set to "true" to use mock data (for testing)
USE_MOCK_DATA=false

# HTTPS port for production (default: 8443)
# Change this if port 8443 is already in use
HTTPS_PORT=8443

